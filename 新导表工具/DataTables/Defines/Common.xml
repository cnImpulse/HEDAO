<module name="">
	<bean name="Range" sep=",">
        <var name="Min" type="int"/>
        <var name="Max" type="int"/>

        <mapper target="client" codeTarget="cs-bin">
            <option name="type" value="HEDAO.Range"/>
            <option name="constructor" value="ExternalTypeUtil.NewRange"/>
        </mapper>
    </bean>

    <bean name="ItemCfg">
        <var name="Id" type="int"/>
        <var name="Name" type="string"/>
        <var name="EffectList" type="list,int"/>
    </bean>

    <bean name="MaterialCfg" parent="ItemCfg"  alias="材料">

    </bean>

    <bean name="EquipCfg" parent="ItemCfg" alias="装备">
        <var name="EquipType" type="EEquipType"/>
        <var name="AtkRange" type="Range"/>
    </bean>
</module>